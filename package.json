{
  "name": "lib-starter",
  "version": "0.0.0",
  "packageManager": "^pnpm@7.14.0",
  "description": "boilerplate for your next TypeScript library",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "author": "Charlie <hi@charchamp.cc> (https://charchamp.cc)",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md --fix",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@charchamp/eslint-config": "^0.0.1",
    "@charchamp/prettier-config": "^0.0.1",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vitest": "^0.24.5"
  }
}
